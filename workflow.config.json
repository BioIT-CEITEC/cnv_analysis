{
  "workflow_description": {
    "name":"structural_var_call",
    "version": 1.0,
    "label":"Structural variant calling",
    "type":"var_call",
    "inputs":[
      "mapped/{sample}.bam*"
    ],
    "outputs":[
      "variant_calls/*",
      "final_CNV_calls/*",
      "cnv_sv/final_report.html"
    ],
    "report_index": "cnv_sv/final_report.html",
    "reports":[
      "cnv_sv/final_report.html"
    ]
  },
  "general_params": [
    "entity_name",
    "sample_name"
  ],
  "requested_params": [
    "reference",
    "lib_ROI"
  ],
  "gui_params" : {
    "primary": {
      "tumor_normal_paired": {
        "label": "tumor/normal paired samples",
        "type": "bool",
        "default": false
      }
    },
    "detailed": {
      "min_variant_frequency": {
        "label": "Fake param",
        "type": "int",
        "default": 0
      }
    },
    "samples": {
      "donor": {
        "label": "Patient ID",
        "type": "string",
        "default": ""
      },
      "tumor_normal": {
        "label": "Tumor/normal",
        "type": "enum",
        "default": "normal",
        "list": {
          "tumor": "tumor",
          "normal": "normal"
        }
      }
    }
  }
}

